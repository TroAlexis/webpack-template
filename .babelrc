{
  "plugins": [
    ["module-resolver", {
      "root": "./src",
      "alias": {
        "Scss": "./src/assets/scss"
      },
      "extensions": [".js", ".jsx", ".scss"]
    }],
    ["react-css-modules", {
      "exclude": "node_modules",
      "generateScopedName": "[name]_[local]_[hash:base64:5]",
      "filetypes": {
        ".scss": {
          "syntax": "postcss-scss"
        }
      },
      "webpackHotModuleReloading": true,
      "handleMissingStyleName": "warn"
    }],
    ["transform-react-pug", {
      "classAttribute": "styleName"
    }],

    "transform-class-properties"
  ],
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react"
  ],
  "env": {
    "production": {
      "plugins": [
        ["react-css-modules", {
          "exclude": "node_modules",
          "generateScopedName": "[hash:base64:5]",
          "filetypes": {
            ".scss": {
              "syntax": "postcss-scss"
            }
          },
          "webpackHotModuleReloading": true
        }]
      ]
    },
    "development": {
      "plugins": [
        "react-refresh/babel"
      ]
    }
  }
}
